<div class="survey-attempt-form">
<h2>Surveys</h2>
<p class="survey-title"><% if can_administer? %>
    <%= link_to "New Survey", new_survey_path %>
  <% end %>
</p>
<div>
  <h4><%= link_to t('Back to home'), '/' %></h4>
  <h4><%= link_to t('Export All'), export_surveys_path() %></h4>
  <h4><%= link_to t('Import'), import_surveys_path() %></h4>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Survey</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "survey", collection: @surveys %>
  </tbody>
</table>
</div>
<% if defined?(Kaminari) %>
  <%= paginate @surveys %>
<% end %>
